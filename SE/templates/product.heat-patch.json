        },
        "e6c52669-c044-4fa7-aa95-0da95c7a8c11": {
          "type": "question",
          "settings": {
            "title": "Kan gravida använda denna?",
            "text": "<p>Produkten är inte testad på gravida. Därför ska produkten ej användas vid graviditet.<\/p>"
          }
        }
