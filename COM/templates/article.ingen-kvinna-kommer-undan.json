    "849546fc-276b-42f5-a52e-56a19502b1f7": {
      "type": "rich-text",
      "blocks": {
        "template--15679057297564__849546fc-276b-42f5-a52e-56a19502b1f7-167024645562938780-0": {
          "type": "heading",
          "settings": {
            "heading": "LÅT OSS BÖRJA FRÅN BÖRJAN. VAD ÄR ENS KLIMAKTERIET?",
            "heading_size": "h2"
          }
        }
      },
      "block_order": [
        "template--15679057297564__849546fc-276b-42f5-a52e-56a19502b1f7-167024645562938780-0"
      ],
      "settings": {
        "text_alignment": "left",
