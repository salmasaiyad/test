          "settings": {
            "heading": "Varför kan jag uppleva lukt från intimområdet?",
            "heading_size": "h2"
          }
        },
        "template--15679215141020__acb3ff06-5131-4d68-ad0c-f98aadbad6e3-1669131050bd0b90a6-1": {
          "type": "text",
          "settings": {
            "text": "<p>Det finns olika faktorer till varför lukt uppkommer från intimområdet, här är några av de vanligaste orsakerna:<\/p>",
            "text_size": "typeset2",
            "secondary_color": false
          }
        }
      },
